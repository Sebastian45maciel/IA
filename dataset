"""

# Parsing de datos
matches = re.split(r'(\d+)(Generacion Z|Frankenstein)', raw_data)
records = []
for i in range(1, len(matches), 3):
    if i+2 < len(matches):
        rid = matches[i]
        category = matches[i+1]
        content = matches[i+2]
        date_match = re.search(r'(\d{1,2}/\d{1,2}/\d{4})', content)
        if date_match:
            date = date_match.group(1)
            records.append({
                'ID': int(rid),
                'Categoria': category,
                'Fecha': date
            })
df = pd.DataFrame(records)
df = df[df['ID'] <= 100] # Limpiar duplicados
df['Fecha'] = pd.to_datetime(df['Fecha'])

# 2. CREACIÓN DE GRÁFICAS

# Ajustar estilos
sns.set_theme(style="whitegrid")

# Gráfica 1: Línea de Tiempo Comparativa (Line Plot)
plt.figure(figsize=(12, 6))
daily_counts = df.groupby(['Fecha', 'Categoria']).size().unstack(fill_value=0)
# Reindexar para llenar huecos de fechas si es necesario, aunque el plot lo maneja bien
# Aseguramos que el eje X sea legible
ax1 = daily_counts.plot(kind='line', marker='o', figsize=(12,6), linewidth=2)
plt.title('Evolución de la Cobertura Mediática: Protestas vs Cine (Agosto - Noviembre 2025)', fontsize=14)
plt.xlabel('Fecha', fontsize=12)
plt.ylabel('Cantidad de menciones/noticias', fontsize=12)
plt.grid(True, linestyle='--', alpha=0.7)
plt.legend(title='Tema')
plt.tight_layout()
plt.savefig('grafica_linea_tiempo.png')
plt.close()

# Gráfica 2: Distribución Total de Menciones (Bar Chart Simple)
plt.figure(figsize=(8, 6))
total_counts = df['Categoria'].value_counts()
colors = ['#ff9999','#66b3ff'] # Colores suaves
ax2 = total_counts.plot(kind='bar', color=colors, rot=0)
plt.title('Volumen Total de Conversación por Tema', fontsize=14)
plt.xlabel('Categoría', fontsize=12)
plt.ylabel('Total de Registros', fontsize=12)
# Añadir etiquetas de valor
for i, v in enumerate(total_counts):
    ax2.text(i, v + 0.5, str(v), ha='center', fontweight='bold')
plt.tight_layout()
plt.savefig('grafica_barras_total.png')
plt.close()

# Gráfica 3: Intensidad en la "Semana Crítica" (Zoom a Noviembre)
# Filtramos solo noviembre para ver el detalle día a día
df_nov = df[df['Fecha'] >= '2025-11-01']
daily_nov = df_nov.groupby(['Fecha', 'Categoria']).size().unstack(fill_value=0)

plt.figure(figsize=(12, 6))
daily_nov.plot(kind='bar', stacked=True, width=0.8, color=['#ff9999','#66b3ff'])
plt.title('Zoom: La "Semana Crítica" de Noviembre 2025', fontsize=14)
plt.xlabel('Fecha', fontsize=12)
plt.ylabel('Cantidad de Noticias', fontsize=12)
plt.legend(title='Tema')
plt.xticks(rotation=45)
plt.tight_layout()
plt.savefig('grafica_barras_apiladas_noviembre.png')
plt.close()

print("Gráficas generadas exitosamente.")
